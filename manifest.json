{
    "name": "Cryptocurrency Price Window",
    "version": "1.0",
    "manifest_version": 1,
    "description": "Replace your new tab page with a list of popular cryptocurrencies and their current prices.",
    "homepage_url": "https://github.com/omeiza/chrome-crypto",
    "icons": {
    	"16": "icons/icon-16.png",
    	"48": "icons/icon-48.png",
    	"128": "icons/icon-128.png"
    },
    "default_locale": "en",
    "chrome_url_overrides": {
    	"newtab": "src/page-override/index.html"
	}
}